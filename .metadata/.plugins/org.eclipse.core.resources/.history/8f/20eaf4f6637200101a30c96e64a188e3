package Locators;

public class StaticEx {
	
	public static void method1() {
		
		System.out.println("calling method1");
	}
	
	public void method2() {
		System.out.println("calling method 2");
	}

	public static void main(String[] args) {
		
		StaticEx s = new StaticEx();
		method1();
		s.method2();
	}

}
